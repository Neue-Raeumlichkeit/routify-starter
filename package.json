{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "npm run build:tailwind && npm run routify -- -b && rollup -c",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "npm run build:tailwind && routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify -D",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "nollup": "jakobrosenberg/nollup#custom-api-fallback",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.2",
    "rollup": "^2.16.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-svelte-hot": "^0.9.2",
    "rollup-plugin-terser": "^6.1.0",
    "spassr": "^1.1.1"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "@sveltech/routify": "^1.8.0-beta.29",
    "postcss-cli": "^7.1.1",
    "rollup-plugin-workbox": "^5.0.1",
    "svelte": "^3.23.2",
    "tailwindcss": "^1.4.6",
    "workbox": "0.0.0"
  }
}
